{
  "name": "calendar",
  "version": "0.1.0",
  "private": true,
  "jest": {
    "automock": false,
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "moduleNameMapper": {
      "^.+\\.css$": "identity-obj-proxy"
    },
    "setupFilesAfterEnv": [
      "./.jest/setup"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    }
  },
  "dependencies": {
    "babel-loader": "^8.1.0",
    "classnames": "^2.2.6",
    "postcss-cli": "^7.1.1",
    "postcss-nested": "^4.2.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "jest",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.10.3",
    "@testing-library/jest-dom": "^5.10.1",
    "@testing-library/react": "^10.4.0",
    "autoprefixer": "^9.8.4",
    "babel-jest": "^24.8.0",
    "identity-obj-proxy": "^3.0.0",
    "jest-emotion": "^10.0.32",
    "jest-fetch-mock": "^3.0.3",
    "npm-run-all": "^4.1.5"
  }
}
